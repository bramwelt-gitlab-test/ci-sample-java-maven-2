<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <id>release</id>
      <username>${env.MAVEN_RELEASE_REPO_USER}</username>
      <password>${env.MAVEN_RELEASE_REPO_PASS}</password>
    </server>
    
  </servers>
  <profiles>
    <profile>
      <id>none</id>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release</id>
          <name>${env.MAVEN_RELEASE_REPO}</name>
          <url>${env.MAVEN_RELEASE_URL}</url>
        </repository>
    
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release</id>
          <name>${env.MAVEN_RELEASE_REPO}</name>
          <url>${env.MAVEN_RELEASE_REPO_URL}</url>
        </pluginRepository>
    
      </pluginRepositories>
    </profile>
    
  </profiles>
  
  <activeProfiles>
      <activeProfile>none</activeProfile>
      
  </activeProfiles>
</settings>
